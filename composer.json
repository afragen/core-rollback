{
    "name": "afragen/core-rollback",
    "description": "Rollback WordPress Core to one of the last several versions.",
    "type": "wordpress-plugin",
    "keywords": [
        "core",
        "rollback",
        "downgrade",
        "upgrade"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Fragen",
            "email": "andy@thefragens.com",
            "homepage": "https://thefragens.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/afragen/core-rollback"
        }
    ],
    "support": {
        "issues": "https://github.com/afragen/core-rollback/issues",
        "source": "https://github.com/afragen/core-rollback"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Fragen\\Rollback\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "wp i18n make-pot . languages/core-rollback.pot"
        ]
    }
}
